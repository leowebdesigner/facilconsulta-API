    {
      "name": "Auth - Login",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "let jsonData = pm.response.json();",
              "if (jsonData?.data?.token) {",
              "    pm.collectionVariables.set(\"token\", jsonData.data.token);",
              "    console.log(\"Token atualizado na variável de coleção.\");",
              "} else {",
              "    console.warn(\"Resposta não contém token.\");",
              "}"
            ],
            "type": "text/javascript"
          }
        }
      ],
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          },
